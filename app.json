{
  "expo": {
    "name": "expo-router-splashscreen",
    "slug": "expo-router-splashscreen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alexanderott.exporoutersplashscreen"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alexanderott.exporoutersplashscreen"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "react-native-bootsplash",
        {
          "android": {
            "parentTheme": "TransparentStatus",
            "darkContentBarsStyle": false
          },
          "assetDir": "src/assets/bootsplash"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
